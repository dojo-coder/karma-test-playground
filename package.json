{
  "name": "test",
  "msw": {
    "workerDirectory": "test"
  },
  "description": "Setup for Mocha unit testing",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js --progress",
    "update": "npx msw init ./ --no-save",
    "build": "webpack --config config/webpack.prod.js",
    "prod": "http-server ./dist -a localhost -p 8000 -o",
    "test:watch": "cross-env REPORTER=mocha karma start --browsers ChromeHeadless karma.conf.js",
    "test": "cross-env COVERAGE=true karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:update": "cross-env COVERAGE=true UPDATE=1 karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:ci": "cross-env REPORTER=html COVERAGE=true karma start --no-fail-on-failing-test-suite --single-run --browsers ChromeHeadless karma.conf.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-arrow-functions": "^7.18.6",
    "@babel/plugin-transform-async-to-generator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@pinia/testing": "^0.0.14",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/test-utils": "^2.0.2",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "chai-karma-snapshot": "^0.8.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-mocha": "^10.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
